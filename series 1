#include<stdio.h>
int main()
{
    int x;
    printf("Enter n value\n");
    scanf("%d",&x);
    int even=5,odd=7;
    for(int i=1;i<=x;i++){
        if(i%2==0){
            printf("%d ",even);
            even+=1;
        }
        else{
            printf("%d ",odd);
            odd+=1;
        }
    }
    return 0;
}
